<% @bookings.each do |booking| %>
booking: <%= booking.id %> <br>
lesson: <%= booking.lesson_id %> <br>
lesson: <%= booking.status %> <br>
<% if booking.status == 'confirmed' %>
  <%= link_to t('.cancel_booking'), cancel_booking_path(booking) %>
  <% end %><br>
<% end %>
<%= link_to t('.new_lesson'), all_lessons_path %>
